" name="mod_chance">4</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunPumpShotgun']" name="mods">partsPumpShotgun_action,partsPumpShotgun_barrel,partsPumpShotgun_magazine,partsPumpShotgun_stock</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunPumpShotgun']" name="mod_chance">4</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunHuntingRifle']" name="mods">partsHuntingRifle_action,partsHuntingRifle_barrel,partsHuntingRifle_stock</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunHuntingRifle']" name="mod_chance">3</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunSMG5']" name="mods">partsSMG5_action,partsSMG5_barrel,partsSMG5_magazine,partsSMG5_stock</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunSMG5']" name="mod_chance">4</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunAK47']" name="mods">partsAK47_action,partsAK47_barrel,partsAK47_magazine,partsAK47_stock</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunAK47']" name="mod_chance">4</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunMR10']" name="mods">partsMR10_action,partsMR10_barrel,partsMR10_magazine,partsMR10_stock</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunMR10']" name="mod_chance">4</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunRocketLauncher']" name="mods">partsRocketLauncher_action,partsRocketLauncher_barrel,partsRocketLauncher_grip</setattribute>	<setattribute xpath="/lootcontainers/lootgroup/item[@name='gunRocketLauncher']" name="mod_chance">3</setattribute> -->	<!-- Loot container tweaks -->	<set xpath="/lootcontainers/lootgroup[@name='backpacks']/item[@group='weaponsBasicGuns+ammo']/@prob">0.3</set>	<append xpath="/lootcontainers/lootgroup[@name='backpacks']">		<item group="weaponsBasicGuns+parts+ammo" prob="0.6" count="1,2"/>	</append>	<!-- safes, weapon caches rare good loot -->	<set xpath="/lootcontainers/lootcontainer[@id='11']/@loot_quality_template">treasureTemplate</set>	<set xpath="/lootcontainers/lootcontainer[@id='11']/item[@group='weaponsBasicGuns+ammo']/@group">weaponsBasicGuns+parts+ammo</set>	<!-- Supply Crate General, the normal airdrop -->	<set xpath="/lootcontainers/lootcontainer[@id='34']/item[@group='weaponsBasicGuns+ammo']/@prob">0.3</set>	<set xpath="/lootcontainers/lootcontainer[@id='34']/item[@group='ammo']/@prob">0.6</set>	<!-- tree hollowed stump, place where people stash things, -->	<set xpath="/lootcontainers/lootcontainer[@id='35']/item[@group='weaponsBasicGuns+ammo']/@prob">0.5</set>	<!-- cntLootChestHero, fancy chest -->	<set xpath="/lootcontainers/lootcontainer[@id='41']/@loot_quality_template">treasureTemplate</set>	<set xpath="/lootcontainers/lootcontainer[@id='41']/item[@group='weaponsBasicGuns+ammo']/@prob">0.5</set>	<append xpath="/lootcontainers/lootcontainer[@id='41']">		<item group="weaponsBasicGuns+parts+ammo" prob="0.75"/>		<item group="weaponsMilitaryGuns+parts+ammo" loot_prob_template="probTemplate1.0"/>	</append>	<!-- gun safe -->	<remove xpath="/lootcontainers/lootcontainer[@id='42']/item[@group='ammoSteel']"/>	<set xpath="/lootcontainers/lootcontainer[@id='42']/@loot_quality_template">treasureTemplate</set>	<setattribute xpath="/lootcontainers/lootcontainer[@id='42']/item[@group='ammo']" name="prob">0.5</setattribute>	<set xpath="/lootcontainers/lootcontainer[@id='42']/item[@group='ammo']/@count">4,6</set>	<append xpath="/lootcontainers/lootcontainer[@id='42']">		<item group="weaponsBasicGuns+parts+ammo" count="1,2" prob="0.25"/>	</append>	<!-- shotgun Messiah -->	<set xpath="/lootcontainers/lootcontainer[@id='50']/@count">2,3</set>	<set xpath="/lootcontainers/lootcontainer[@id='50']/@loot_quality_template">treasureTemplate</set>	<set xpath="/lootcontainers/lootcontainer[@id='50']/item[@group='weaponsBasicGuns+ammo']/@group">weaponsCommonParts</set>	<set xpath="/lootcontainers/lootcontainer[@id='50']/item[@group='weaponsMilitaryGuns+ammo']/@group">weaponsMilitaryParts</set>	<append xpath="/lootcontainers/lootcontainer[@id='50']">		<item group="weaponsBasicGuns+ammo" loot_prob_template="probTemplate1.0"/>		<item group="weaponsMilitaryGuns+ammo" loot_prob_template="probTemplate0.75"/>	</append>	<!-- cntLootCrateHero, Hero / Hidden Stash Cardboard Loot-->	<set xpath="/lootcontainers/lootcontainer[@id='95']/@loot_quality_template">treasureTemplate</set>	<append xpath="/lootcontainers/lootcontainer[@id='95']">		<item group="weaponsBasicGuns+parts+ammo" count="1,2"/>		<item group="weaponsMilitaryGuns+parts+ammo" loot_prob_template="probTemplate1.0"/>	</append>	<!-- cntWeaponsBagSmall -->	<set xpath="/lootcontainers/lootcontainer[@id='100']/@loot_quality_template">treasureTemplate</set>	<set xpath="/lootcontainers/lootcontainer[@id='100']/item[@group='weaponsPistol+ammo']/@prob">1</set>	<append xpath="/lootcontainers/lootcontainer[@id='100']">		<item group="partsPistol" prob="1.5" count="1,2"/>		<item group="parts44Magnum" loot_prob_template="probTemplate0.5" count="1,2"/>		<item group="partsSMG5" loot_prob_template="probTemplate0.5" count="1,2"/>	</append>	<!-- cntWeaponsBagLarge -->	<set xpath="/lootcontainers/lootcontainer[@id='101']/@loot_quality_template">treasureTemplate</set>	<set xpath="/lootcontainers/lootcontainer[@id='101']/item[@group='weaponsBasicGuns+ammo']/@prob">1</set>	<append xpath="/lootcontainers/lootcontainer[@id='101']">		<item group="weaponsBasicGuns+parts+ammo" prob="1.5"/>		<item group="weaponsMilitaryGuns+parts+ammo" loot_prob_template="probTemplate1.0"/>	</append></configs>