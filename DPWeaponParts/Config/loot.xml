<configs>	<insertBefore xpath="/lootcontainers/lootgroup[@name='questChallenge']">		<lootgroup name="partsPistol" count="1">			<item name="Pistol Action"/>			<item name="Pistol Barrel"/>			<item name="Pistol Magazine"/>			<item name="Pistol Grip"/>			<item name="gunPistol" prob="0.5"/>		</lootgroup>		<lootgroup name="partsPumpShotgun" count="1">			<item name="Shotgun Action"/>			<item name="Shotgun Barrel"/>			<item name="Shotgun Magazine"/>			<item name="Shotgun Stock"/>			<item name="gunPumpShotgun" prob="0.5"/>		</lootgroup>		<lootgroup name="partsHuntingRifle" count="1">			<item name="Hunting Rifle Action"/>			<item name="Hunting Rifle Barrel"/>			<item name="Hunting Rifle Stock"/>			<item name="gunHuntingRifle" prob="0.5"/>		</lootgroup>		<lootgroup name="parts44Magnum" count="1">			<item name="44 Magnum Action"/>			<item name="44 Magnum Barrel"/>			<item name="44 Magnum Cylinder"/>			<item name="44 Magnum Grip"/>			<item name="gun44Magnum" prob="0.5"/>		</lootgroup>		<lootgroup name="partsSMG5" count="1">			<item name="SMG5 Action"/>			<item name="SMG5 Barrel"/>			<item name="SMG5 Magazine"/>			<item name="SMG5 Stock"/>			<item name="gunSMG5" prob="0.5"/>		</lootgroup>		<lootgroup name="partsAK47" count="1">			<item name="AK47 Action"/>			<item name="AK47 Barrel"/>			<item name="AK47 Magazine"/>			<item name="AK47 Stock"/>			<item name="gunAK47" prob="0.5"/>		</lootgroup>		<lootgroup name="partsMR10" count="1">			<item name="Sniper Rifle Action"/>			<item name="Sniper Rifle Barrel"/>			<item name="Sniper Rifle Magazine"/>			<item name="Sniper Rifle Stock"/>			<item name="gunMR10" prob="0.5"/>		</lootgroup>		<lootgroup name="partsRocketLauncher" count="1">			<item name="Rocket Launcher Action"/>			<item name="Rocket Launcher Barrel"/>			<item name="Rocket Launcher Grip"/>			<item name="gunRocketLauncher" prob="0.5"/>		</lootgroup>		<lootgroup name="weaponsCommonParts" count="1" loot_quality_template="treasureTemplate">			<item group="partsPistol"/>			<item group="partsPumpShotgun"/>			<item group="partsHuntingRifle"/>			<item group="parts44Magnum" loot_prob_template="probTemplate0.5"/>		</lootgroup>		<lootgroup name="weaponsMilitaryParts" count="1" loot_quality_template="treasureTemplate">			<item group="partsSMG5"/>			<item group="partsAK47"/>			<item group="partsMR10"/>			<item group="partsRocketLauncher" loot_prob_template="probTemplate0.5"/>		</lootgroup>	</insertBefore>	<insertAfter xpath="/lootcontainers/lootgroup[@name='weaponsMilitaryGuns+ammo']">		<lootgroup name="weaponsBasicGuns+parts+ammo">			<item group="weaponsCommonParts" prob="0.86" count="1,2"/>			<item group="weaponsPistol+ammo" prob="0.025"/>			<item group="weaponsShotgun+ammo" prob="0.025"/>			<item group="weaponsHuntingRifle+ammo" prob="0.025"/>			<item group="weaponsMagnum+ammo" prob="0.015"/>			<item name="gunCompoundBow" prob="0.05"/>		</lootgroup>		<lootgroup name="weaponsMilitaryGuns+parts+ammo">			<item group="weaponsMilitaryParts" prob="0.86" count="1,2"/>			<item group="weaponsMP5+ammo" prob="0.04"/>			<item group="weaponsAK47+ammo" prob="0.04"/>			<item group="weaponsSniperRifle+ammo" prob="0.04"/>			<item group="weaponsRocketLauncher+ammo" prob="0.02"/>		</lootgroup>	</insertAfter>	<!-- Add chance to spawn parts alongside weapon -->	<append xpath="/lootcontainers/lootgroup[@name='weaponsPistol+ammo']">		<item name="Pistol Action"/>		<item name="Pistol Barrel"/>		<item name="Pistol Magazine"/>		<item name="Pistol Grip"/>	</append>	<append xpath="/lootcontainers/lootgroup[@name='weaponsShotgun+ammo']">		<item name="Shotgun Action"/>		<item name="Shotgun Barrel"/>		<item name="Shotgun Magazine"/>		<item name="Shotgun Stock"/>	</append>	<append xpath="/lootcontainers/lootgroup[@name='weaponsHuntingRifle+ammo']">		<item name="Hunting Rifle Action"/>		<item name="Hunting Rifle Barrel"/>		<item name="Hunting Rifle Stock"/>	</append